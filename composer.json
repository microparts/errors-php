{
  "name": "microparts/errors-php",
  "description": "Is a complex solution to handling exceptions for any web application.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "roquie",
      "email": "roquie0@gmail.com"
    }
